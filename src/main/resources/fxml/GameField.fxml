<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<VBox styleClass="rootNode" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.121"
      stylesheets="/css/game.css">
    <HBox minHeight="30">
        <Button onAction="#toMenu">Exit to menu</Button>
    </HBox>
    <HBox minHeight="20" alignment="CENTER">
        <Label styleClass="gameoverLabel" fx:id="gameoverLabel"/>
    </HBox>
    <ScrollPane styleClass="gamePaneWrapper" VBox.vgrow="ALWAYS" fitToWidth="true" fitToHeight="true">
        <padding>
            <Insets top="10" right="10" bottom="10" left="10"/>
        </padding>
        <VBox styleClass="gamePane" fx:id="gamePane" spacing="2" alignment="CENTER">

        </VBox>
    </ScrollPane>
    <HBox minHeight="50">

    </HBox>
</VBox>
